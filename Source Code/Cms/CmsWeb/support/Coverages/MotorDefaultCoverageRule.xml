		<!-- Indiana -->
		<Rule Action="Master" StartDate="1950-01-01" EndDate="3000-12-12" STATE_ID="14">
			<Conditions>
				<!--Symbol-->
				<Condition Operand1="1" Operator="==" Operand2="1" OperandType="Double">
					<ToSet Key="$TEMP_DCOLL" Value="0"></ToSet>
					<ToSet Key="$TEMP_DCOMP" Value="0"></ToSet>
				</Condition>
			</Conditions>
			<Conditions>
				<!--Symbol-->
				<ToSet Key="$TEMP_DCOLL" Value="0"></ToSet>
				<Condition Operand1="$SYMBOL" Operator="&gt;=" Operand2="1" OperandType="Double">
					<SubConditions>
						<SubCondition Operand1="$SYMBOL" Operator="&lt;" Operand2="3" OperandType="Double">
							<ToSet Key="$TEMP_DCOMP" Value="100"></ToSet>
							<ToSet Key="$TEMP_DCOLL" Value="100"></ToSet>
						</SubCondition>
						<SubCondition Operand1="$SYMBOL" Operator="&lt;" Operand2="6" OperandType="Double">
							<ToSet Key="$TEMP_DCOMP" Value="250"></ToSet>
							<ToSet Key="$TEMP_DCOLL" Value="250"></ToSet>
						</SubCondition>
						<SubCondition Operand1="$SYMBOL" Operator="&lt;" Operand2="9" OperandType="Double">
							<ToSet Key="$TEMP_DCOMP" Value="250"></ToSet>
							<ToSet Key="$TEMP_DCOLL" Value="500"></ToSet>
						</SubCondition>
						<SubCondition Operand1="$SYMBOL" Operator="&gt;=" Operand2="9" OperandType="Double">
							<ToSet Key="$TEMP_DCOMP" Value="1000"></ToSet>
							<ToSet Key="$TEMP_DCOLL" Value="1000"></ToSet>
						</SubCondition>
					</SubConditions>
				</Condition>
			</Conditions>
			<Conditions>
				<!--MOTORCYCLE TYPE 11424-->
