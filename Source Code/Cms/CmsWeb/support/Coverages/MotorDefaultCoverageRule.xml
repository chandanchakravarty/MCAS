							<ToSet Key="$TEMP_DCOMP" Value="500"></ToSet>
							<ToSet Key="$TEMP_DCOLL" Value="500"></ToSet>
						</SubCondition>
						<SubCondition Operand1="$SYMBOL" Operator="&gt;=" Operand2="9" OperandType="Double">
							<ToSet Key="$TEMP_DCOLL" Value="1000"></ToSet>
						</SubCondition>
					</SubConditions>
				</Condition>
			</Conditions>
			<Conditions>
				<!--Set Value Of Comprehensive-->
				<Condition Operand1="$TEMP_DCOMP" Operator="&gt;" Operand2="$DEDUCTIBLE_COMP" OperandType="Double">
					<ToSet Key="$DEDUCTIBLE_COMP" Value="$TEMP_DCOMP"></ToSet>
				</Condition>
			</Conditions>
			<Conditions>
				<!--Set Value Of Collosion-->
