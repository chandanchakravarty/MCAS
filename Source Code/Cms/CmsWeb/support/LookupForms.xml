	<AddlInterest>
		<Select>
      SELECT	A.HOLDER_ID, A.HOLDER_NAME
      FROM MNT_HOLDER_INTEREST_LIST A WITH(NOLOCK)
    </Select>
		<WhereColumns>
			<column>
				<index>1</index>
				<name>
          IsNull(A.HOLDER_ID,0)  NOT IN
          (SELECT ISNULL(HOLDER_ID,0) FROM POL_ADD_OTHER_INT WITH(NOLOCK) WHERE VEHICLE_ID)
        </name>
				<type>string</type>
			</column>
		</WhereColumns>
		<OrderBy>HOLDER_NAME ASC
		</OrderBy>
		<GridColumns>
			<column>
				<datafield>HOLDER_NAME</datafield>
				<heading>Holder Name</heading>
				<dataformatstring>None</dataformatstring>
				<sorting>false</sorting>
				<visible>true</visible>		
			</column>		
		</GridColumns>
		<SearchColumns>
			<column>
				<name>HOLDER_NAME</name>	
				<heading>Holder Name</heading>
				<type>string</type>
			</column>
		</SearchColumns>	
	</AddlInterest>
