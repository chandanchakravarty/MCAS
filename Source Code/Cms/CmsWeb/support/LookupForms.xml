<?xml version="1.0" encoding="utf-8" ?> 
<LookupForms>
<TransactionCodes>
		<Select>
      SELECT TOP 50 TRAN_CODE,
      convert(varchar,TRAN_ID) + ',' + TRAN_CODE as TRAN_CODE1,
      DISPLAY_DESCRIPTION
      FROM ACT_TRANSACTION_CODES WITH(NOLOCK)
    </Select>
		<OrderBy>DISPLAY_DESCRIPTION ASC</OrderBy>
		<GridColumns>
		<column>
				<datafield>TRAN_CODE1</datafield>
				<heading>Transaction Code</heading>
				<dataformatstring>None</dataformatstring>
				<sorting>false</sorting>
				<visible>false</visible>		
			</column>
			<column>
				<datafield>TRAN_CODE</datafield>
				<heading>Transaction Code</heading>
				<dataformatstring>None</dataformatstring>
				<sorting>false</sorting>
				<visible>true</visible>		
			</column>
			<column>
				<datafield>DISPLAY_DESCRIPTION</datafield>
				<heading>Transaction Code Description</heading>
				<dataformatstring>None</dataformatstring>
				<sorting>false</sorting>
				<visible>true</visible>
			</column>			
		</GridColumns>
		<SearchColumns>
			<column>
				<name>DISPLAY_DESCRIPTION</name>	
				<heading>Transaction Code Description</heading>
				<type>string</type>
			</column>
			<column>
				<name>TRAN_CODE</name>	
				<heading>Transaction Code</heading>
				<type>string</type>
			</column>
		</SearchColumns>	
	</TransactionCodes>
	<CustLookupForm>
		<Select>
