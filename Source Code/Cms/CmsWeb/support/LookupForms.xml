			</column>
			<column>
				<name>Customer_Code</name>	
				<heading>Customer Code</heading>
				<type>string</type>
			</column>
		</SearchColumns>
		<GridColumns>
			<column>
				<datafield>CUSTOMER_ID</datafield>
				<heading>Customer ID</heading>
