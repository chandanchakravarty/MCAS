		<CustAndStateLookupForm>
		<SQL>
      SELECT cast(CUSTOMER_ID as varchar(20))+'^'+isnull(customer_state,'') as CUSTOMER_ID , CUSTOMER_CODE, CUSTOMER_FIRST_NAME + ' ' + CUSTOMER_LAST_NAME as Name
      FROM CLT_CUSTOMER_LIST WITH(NOLOCK)
    </SQL>
