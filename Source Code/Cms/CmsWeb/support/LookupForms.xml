				<visible>true</visible>
			</column>
		</GridColumns>
	</CustLookupForm>	
	<LOOKUPFORM>
		<Select>
      SELECT TOP 50 LOOKUP_UNIQUE_ID, LOOKUP_VALUE_DESC FROM MNT_LOOKUP_VALUES MLV WITH(NOLOCK)
      INNER JOIN MNT_LOOKUP_TABLES MLT WITH(NOLOCK) ON
      MLV.LOOKUP_ID = MLT.LOOKUP_ID
    </Select>
		<WhereColumns>
			<column>
				<index>1</index>
				<name>MLT.LOOKUP_NAME</name>
				<type>string</type>
			</column>
		</WhereColumns>
		<OrderBy>LOOKUP_VALUE_DESC ASC
		</OrderBy>
		<GridColumns>
			<column>
				<datafield>LOOKUP_UNIQUE_ID</datafield>
				<heading>Vendor ID</heading>
				<dataformatstring>None</dataformatstring>
				<sorting>false</sorting>
				<visible>false</visible>		
			</column>
			<column>
				<datafield>LOOKUP_VALUE_DESC</datafield>
				<heading>Desc</heading>
				<dataformatstring>None</dataformatstring>
				<sorting>false</sorting>
				<visible>true</visible>
			</column>			
		</GridColumns>
		<SearchColumns>
			<column>
				<name>LOOKUP_VALUE_DESC</name>	
				<heading>Desc</heading>
				<type>string</type>
			</column>
		</SearchColumns>		
	</LOOKUPFORM>	
