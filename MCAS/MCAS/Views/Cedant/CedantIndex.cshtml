@model IEnumerable<MCAS.Web.Objects.MastersHelper.CedantModel>
@using MCAS.Globalisation
@{
    ViewBag.Title = "CedantIndex";
    Layout = "~/Views/Shared/_SystemAdminLayout.cshtml";
    @Html.Hidden("oldgroup", 1)
    MCAS.Web.Objects.CommonHelper.SecurityPermissions myPemissions = (MCAS.Web.Objects.CommonHelper.SecurityPermissions)ViewData["UserPermissions"];
    System.Resources.ResourceManager PageResource = (new MCAS.Web.Objects.MastersHelper.TransactionModel()).GetResourceManager(Convert.ToString(ViewData["PageResource"]));
}
<script src='@Url.Content("~/Scripts/jquery-2.1.0.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Scripts/jquery.validate.min.js")' type="text/javascript"></script>
<script src='@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")' type="text/javascript"></script>
<div class="dashboard-wrapper">
    <div class="container">
        <div class="row page-title">
            <div class="col-lg-12">
                <h2>@MCAS.Web.Objects.Resources.Common.Common.SystemAdmin</h2>
                <ul class="breadcrumb">
                    <li>@PageResource.GetString("ServiceProviderMaster")</li>
                    <li>@PageResource.GetString("InsurerMaster")</li>
                </ul>
            </div>
        </div>
        <hr />
        <div class="row">
            <div class="col-lg-12" style="text-align:left">
                <div class="btn-group less-bottom-margin">
                    <a class="btn btn-info" href='@Url.Action("CedantIndex", "Cedant")'>@PageResource.GetString("InsurerMaster")</a>
                    <a class="btn btn-default" href='@Url.Action("SurveyorIndex", "AdjusterMasters")'> @PageResource.GetString("SurveyorMaster")</a> 
                    <a class="btn btn-default" href='@Url.Action("SolicitorIndex", "AdjusterMasters")'>@PageResource.GetString("LawyerMaster")</a> 
                    <a class="btn btn-default" href='@Url.Action("DepotMasterIndex", "Masters")'>@PageResource.GetString("WorkshopDepotMaster")</a>
                    <a class="btn btn-default" href='@Url.Action("HospitalIndex", "ClaimMasters")'>@PageResource.GetString("HospitalMaster")</a> 
                    <a class="btn btn-default" href='@Url.Action("AdjusterIndex", "AdjusterMasters")'>@PageResource.GetString("AdjusterMaster")</a>
                </div>
            </div>
        </div>
        <hr/>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sx-12 col-sm-12">
                <div class="panel panel-secondary">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="panel panel-info">
                                <div class="panel-heading clearfix">
                                    <i class="icon-check-sign"></i>
                                    <h3 class="panel-title">
                                         @PageResource.GetString("InsurerMaster")
                                    </h3>
                                </div>
                                 <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="panel panel-info">
                                <div class="panel-sub-heading">
                                    <span class="text-danger">*</span>@PageResource.GetString("SearchScreen")
                                </div>
                                <div class="panel-body">
                                    @using (Html.BeginForm("CedantIndex", "Cedant", FormMethod.Post, new { role = "form", @class = "form-horizontal" }))
                                    {
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="form-group">
                                                    <label class="col-sm-4">
                                                        @PageResource.GetString("CedantName")</label>
                                                    <div class="col-sm-6">
                                                        <input type="text" placeholder="" value="@ViewBag.cedantname" name="inputCedantName" id="inputCedantName" class="form-control"/>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="row top-buffer">
                                        </div>
                                        <div class="row top-buffer">
                                        </div> 
                                        <div class="row">
                                            <div class="col-lg-12 text-center">
                                                <button type="submit" class="btn btn-info">
                                                    @PageResource.GetString("Search")</button>
                                                &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
                                                @if (@myPemissions.Write)
                                                {
                                                    @Html.EncodedActionLink(@PageResource.GetString("AddNew"), "CedantEditor", "Cedant",new{}, new { @class = "btn btn-info" })
                                            
                                             
                                                }
                                            </div>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <h4 class="text-info">@PageResource.GetString("SearchResults")</h4>
                            <hr/>
                        </div>
                        <div class="col-lg-12">
                            <div class="table-responsive">
                            <div class="row-fluid">
                             <div class="dataTables_wrapper" id="Diary_wrapper" role="grid">
                                <table id="tableDiarySrc" width="100%" class="table table-striped table-hover overflow:auto; display ">
                                    <thead>
                                        <tr>
                                            <th style="width: 10%;;padding-left:9px">
                                                @PageResource.GetString("CedantCode")
                                            </th>
                                            <th style="width: 50%;;padding-left:9px">
                                                @PageResource.GetString("CedantName")
                                            </th>
                                            <th style="width: 20%;;padding-left:9px">
                                                @PageResource.GetString("TelephoneNoOff")
                                            </th>
                                            <th style="width: 20%;">
                                                <span class="block text-right">@PageResource.GetString("Select")</span>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody style="text-transform: capitalize;">
                                        @{
                                            int trid = 1;
                                            int rowmatch = 0;
                                            var display = "";

                                            foreach (var item in Model)
                                            {
                                                rowmatch += 1;
                                            <tr group='@(trid)' style="display:@display;">
                                                <td style="width: 10%;word-break: break-all;">
                                                    @Html.DisplayFor(modelItem => item.CedantCode)
                                                </td>
                                                <td style="width: 50%;word-break: break-all;">
                                                    @Html.DisplayFor(modelItem => item.CedantAddress.InsurerName)
                                                </td>
                                                <td style="width: 20%;word-break: break-all;">
                                                    @Html.DisplayFor(modelitem => item.CedantAddress.OffNo1)
                                                </td>
                                                <td style="width: 20%;word-break: break-all;">
                                                    @if (@myPemissions.Write)
                                                    {
                                                        @Html.EncodedActionLink(@PageResource.GetString("Edit"), "CedantEditor", new { CedantId = item.CedantId, pageMode = "Edit" }, new { @class = "btn btn-xs btn-warning" })

                                                    }
                                                    @if (@myPemissions.Read)
                                                    {
                                                        @Html.EncodedActionLink(@PageResource.GetString("View"), "CedantEditor", new { CedantId = item.CedantId, pageMode = "View" }, new { @class = "btn btn-xs btn-info", id = "divView" })    
                                                    }
                                                </td>
                                            </tr>
                                                   
                                            }}
                                    </tbody>
                                </table>
                                </div>
                                </div>
                            </div>
                        </div>
                       
                    </div>
                    </div>
                </div>
            </div>
            </div>
                        </div>
                    </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    var do_on_load = function () {
        $(document).on("focus", ":input", function () {
            $(this).attr("autocomplete", "off")
        });
        loadGroupStatus();
    };
    $(document).ready(do_on_load);
    $(window).bind('page:change', do_on_load);
    function loadCountryList() {
        $.ajax({
            url: '@(Url.Action("FillCountryCodeList"))',
            cache: !1,
            success: function (a) {
                $("#ddlCountry").empty();
                $.each(a, function (a, b) {
                    $("#ddlCountry").append("<option value='" + b.CountryShortCode + "'>" + b.CountryName + "</option>")
                })
            },
            error: function (a) {
                alert("error : " + a)
            }
        })
    };
    function loadGroupStatus() {
        $.ajax({
            url: '@(Url.Action("FillGeneralStatus"))',
            cache: !1,
            success: function (a) {
                $("#ddlStatus").empty();
                $.each(a, function (a, b) {
                    $("#ddlStatus").append("<option value='" + b.Lookup_value + "'>" + b.Lookup_desc + "</option>")
                })
            },
            error: function (a) {
                alert("error : " + a)
            }
        })
    };
    function Pagination(c) {
        var a = Math.ceil(parseFloat('@Model.Count()' / 10));
        "linext" == c && (c = "li" + (parseInt($("#oldgroup").val()) == a ? a : parseInt($("#oldgroup").val()) + 1));
        "liprevious" == c && (c = "li" + (1 == parseInt($("#oldgroup").val()) ? 1 : parseInt($("#oldgroup").val()) - 1));
        var d = $("#oldgroup").val(),
        b = c.replace("li", "");
        $("#oldgroup").val(b);
        $("table tr[group=" + d + "]").css("display", "none");
        $("table tr[group=" + b + "]").css("display", "");
        $(".active").removeClass();
        $("#" + c).addClass("active");
        if (0 < a) {
            for (i = 1; i <= a; i++) $("#li" + i).css("display", "none");
            if (1 < b) if (parseFloat(b) + 2 <= parseFloat(a)) for (i = parseFloat(2 == b ? 3 : b) - 2; i <= parseFloat(2 == b ? 3 : b) + 2; i++) i <= a && $("#li" + i).css("display", "");
            else for (i = parseFloat(a) - 4; i <= parseFloat(a); i++) $("#li" + i).css("display", "");
            else for (i = 1; 5 >= i; i++) $("#li" + i).css("display", "")
        }
    };
</script>
