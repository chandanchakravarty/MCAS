@model IEnumerable<MCAS.Web.Objects.ClaimObjectHelper.TacFileUploadViewListModel>
@using MCAS.Web.Objects.Resources.ClaimUploadProcessing;
@using MCAS.Web.Objects.Resources.Common;
@using MCAS.Web.Objects.CommonHelper
@using MCAS.Globalisation
@{
    @Html.Hidden("oldgroup", 1)
    Layout = null;
    SecurityPermissions myPemissions = (SecurityPermissions)ViewData["UserPermissions"];
    var isWrite = myPemissions.Write.ToString() == "False" ? "N" : "Y";
    System.Text.StringBuilder emp = new System.Text.StringBuilder();
    System.Text.StringBuilder emp2 = new System.Text.StringBuilder();
    string[] ids = Model.Select(x => x.FileRefNo).Distinct().ToArray();
    for (var i = 0; i < ids.Length; i++)
    {
        int idnew1 = Model.Count(x => x.FileRefNo == ids[i]);
        emp.Append(idnew1);
        if (i != ids.Length - 1)
        {
            emp.Append("~");
        }
    }

    string[] emp1 = emp.ToString().Split('~');
    if (emp.Length != 0)
    {
        for (var k = 0; k < emp1.Length; k++)
        {
            int sum = 0;
            for (var j = 0; j <= Convert.ToInt32(k); j++)
            {
                sum += Convert.ToInt32(emp1[j]);
            }
            emp2.Append(sum);
            emp2.Append("~");
        }
    }
    string avl = emp.Length != 0 ? "0~" + emp2.ToString().Substring(0, emp2.Length - 1) : "";
    string[] val21 = avl.Split('~');
    string stl = Convert.ToString(val21[val21.Length - 1]);
    string[] val1 = val21.Where(o => o != stl).ToArray();
}
<style type="text/css">
#table1 th
{
    word-wrap: break-word;
}
</style>
<div class="panel-body">
    <div class="panel-body">
            <div class="col-lg-12">
                <div class="table-responsive">
                    <table id="table1" class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th rowspan="2" style="width: 2%;">
                                </th>
                                <th rowspan="2" style="width: 11%;">
                                    @TacFileUploadEditor.UploadRefNo.
                                </th>
                                <th rowspan="2" style="width: 11%;">
                                     @TacFileUploadEditor.UploadedDateTime
                                </th>
                                <th colspan="3" style="width: 11%; border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.TotalRecords
                                </th>
                                <th colspan="3" style="width: 13%; border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.SuccessRecords
                                </th>
                                <th colspan="3" style="width: 11%;">
                                       @TacFileUploadEditor.FailedRecords
                                </th>
                                <th rowspan="2" style="width: 11%;">
                                     @TacFileUploadEditor.DownloadClaimFileToUpload
                                </th>
                                <th rowspan="2" style="width: 11%;">
                                     @TacFileUploadEditor.LOGFile
                                </th>
                                <th rowspan="2" style="width: 11%;">
                                     @TacFileUploadEditor.Status
                                </th>
                                <th rowspan="2" style="width: 10%;">
                                     @TacFileUploadEditor.Action
                                </th>
                            </tr>
                            <tr style="border: none;">
                                <th style="border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.IP
                                </th>
                                <th style="border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.Rep
                                </th>
                                <th style="border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.Bus
                                </th>
                                &nbsp;
                                <th style="border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.IP
                                </th>
                                <th style="border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.Rep
                                </th>
                                <th style="border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.Bus
                                </th>
                                &nbsp;
                                <th style="border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.IP
                                </th>
                                <th style="border-right: 1px solid #DDDDDD;">
                                     @TacFileUploadEditor.Rep
                                </th>
                                <th>
                                     @TacFileUploadEditor.Bus
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @{
        int trid = 1;
        int trid1 = 0;
        int rowmatch = 0;
        var display = "";
        var id = 0;
        var id2 = 1;
        var id3 = 0;

        foreach (var item in Model)
        {

            if (Array.IndexOf(val1, Convert.ToString(trid1)) > -1)
            {
                id2 = 1;
                id3++;
            }
            var idn = "l" + id3;
            var idn3 = idn + "-" + id2;

            if (Array.IndexOf(val1, Convert.ToString(trid1)) > -1)
            {
                var totalall = (from l in Model.Where(x => x.FileRefNo == item.FileRefNo) select l.TotalRecords).ToArray();
                var total1 = ""; var total2 = ""; var total3 = "";
                if (totalall.Length == 1)
                {
                    total1 = Convert.ToString(totalall[0]);

                }
                else if (totalall.Length == 2)
                {
                    total1 = Convert.ToString(totalall[0]);
                    total2 = Convert.ToString(totalall[1]);
                }
                else if (totalall.Length == 3)
                {
                    total1 = Convert.ToString(totalall[0]);
                    total2 = Convert.ToString(totalall[1]);
                    total3 = Convert.ToString(totalall[2]);
                }


                var SuccessAll = (from l in Model.Where(x => x.FileRefNo == item.FileRefNo) select l.SuccessRecords).ToArray();

                var Success1 = ""; var Success2 = ""; var Success3 = "";
                if (SuccessAll.Length == 1)
                {
                    Success1 = Convert.ToString(SuccessAll[0]);
                }
                else if (SuccessAll.Length == 2)
                {
                    Success1 = Convert.ToString(SuccessAll[0]);
                    Success2 = Convert.ToString(SuccessAll[1]);
                }
                else if (SuccessAll.Length == 3)
                {
                    Success1 = Convert.ToString(SuccessAll[0]);
                    Success2 = Convert.ToString(SuccessAll[1]);
                    Success3 = Convert.ToString(SuccessAll[2]);

                }


                var FailedAll = (from l in Model.Where(x => x.FileRefNo == item.FileRefNo) select l.FailedRecords).ToArray();
                var Failed1 = ""; var Failed2 = ""; var Failed3 = "";
                if (FailedAll.Length == 1)
                {
                    Failed1 = Convert.ToString(FailedAll[0]);
                }
                else if (FailedAll.Length == 2)
                {
                    Failed1 = Convert.ToString(FailedAll[0]);
                    Failed2 = Convert.ToString(FailedAll[1]);
                }
                else if (FailedAll.Length == 3)
                {
                    Failed1 = Convert.ToString(FailedAll[0]);
                    Failed2 = Convert.ToString(FailedAll[1]);
                    Failed3 = Convert.ToString(FailedAll[2]);
                }
                
                             
                                <tr group='@(trid)'  style="display:@display;">
                                    <td>
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FileRefNo)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.UploadedDate)
                                    </td>
                                    <td>
                                        @total3
                                    </td>
                                    <td>
                                        @total2
                                    </td>
                                    <td>
                                        @total1
                                    </td>
                                    <td>
                                        @Success3
                                    </td>
                                    <td>
                                        @Success2
                                    </td>
                                    <td>
                                        @Success1
                                    </td>
                                    <td>
                                        @Failed3
                                    </td>
                                    <td>
                                        @Failed2
                                    </td>
                                    <td>
                                        @Failed1
                                    </td>
                                    <td>
                                        @if (item.Status == "Success")
                                        {
                                            @Html.ActionLink(TacFileUploadEditor.Download, "FileDownLoader", "ClaimUploadProcessing", new { Fileformat = "CSV", fileRefNo = item.FileRefNo, FileType = "ClaimDownload" }, new { target = "_blank", @class = "btn btn-xs btn-info", dissable = "readonly" })
                                        }
                                    </td>
                                    <td>
                                        @if (item.Status == "Failed")
                                        {
                                            @Html.ActionLink(TacFileUploadEditor.FailedLog, "FileDownLoader", "ClaimUploadProcessing", new { Fileformat = "CSV", fileRefNo = item.FileRefNo, FileType = "LOGFile" }, new { target = "_blank", @class = "btn btn-xs btn-info", dissable = "readonly" })
                                        }
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Status)
                                    </td>
                                    <td>
                                        @if (item.Status == "Failed")
                                        {
                                            @Html.EncodedActionLink(TacFileUploadEditor.ReUpload, "TacFileUploadEditor", "ClaimUploadProcessing", new { FileId = item.FileId, FileRef = item.FileRefNo, claimMode = "Upload", pageMode = "Edit" }, new { @class = "btn btn-xs btn-info", dissable = "readonly" })
                                        }
                                    </td>
                                    <td>
                                    </td>
                                </tr>
                //  id++;
            }



            id2 += 1;
            trid1 += 1;
            rowmatch += 1;
            if (rowmatch > 11)
            {
                trid += 1;

                rowmatch = 0;

                display = "none";
            }
        }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
            @{
        decimal count = (decimal)Model.Count() / 12;
        int totalcount = (int)Math.Ceiling(count);
                <div class="row">
                    <div class="col-lg-1 text-center">
                        <div class="form-group">
                        </div>
                    </div>
                    <div class="col-lg-10 text-center">
                        <ul class="pagination no-margin">
                            <li id="liprevious" onclick="Pagination('liprevious');"><a href="#">«</a> </li>
                            @for (int j = 0; j < totalcount; j++)
                            {
                                if (j == 0)
                                { 
                                <li id="li@(j + 1)" class="active" onclick="Pagination('li@(j + 1)');"><a href="#">@(j + 1)</a>
                                </li>
                                }
                                else
                                {
                                    if (j > 4)
                                    {
                                <li id="li@(j + 1)" style="display: none;" onclick="Pagination('li@(j + 1)');" ><a
                                    href="#">@(j + 1)</a></li>
                                    }
                                    else
                                    { 
                                <li id="li@(j + 1)" onclick="Pagination('li@(j + 1)');" ><a href="#">@(j + 1)</a></li>
                                    }
                                }
                            }
                            <li id="linext" onclick="Pagination('linext');"><a href="#">»</a> </li>
                        </ul>
                    </div>
                    <div class="col-lg-1 text-center">
                        <div class="form-group">
                        </div>
                    </div>
                </div>
            }
        </div>
</div>
<script type="text/javascript">
    var do_on_load = function () {
        '@isWrite' == 'N' && $("#table1 a").each(function () {
            $(this).removeAttr('href')
        });

    };
    $(document).ready(do_on_load);
    $(window).bind('page:change', do_on_load);
    function Pagination(id) {
        var totalcount = Math.ceil(parseFloat('@Model.Count()' / 12));
        if (id == "linext") {
            id = "li" + (parseInt($("#oldgroup").val()) == totalcount ? totalcount : (parseInt($("#oldgroup").val()) + 1));
        }
        if (id == "liprevious") {
            id = "li" + (parseInt($("#oldgroup").val()) == 1 ? 1 : (parseInt($("#oldgroup").val()) - 1));
        }
        var oldgroup_id = $("#oldgroup").val();
        var tabid_id = id.replace("li", "");
        $("#oldgroup").val(tabid_id);
        $("table tr[group=" + oldgroup_id + "]").css("display", "none");
        $("table tr[group=" + tabid_id + "]").css("display", "");
        $('.active').removeClass();
        $('#' + id).addClass("active");

        if (totalcount > 0) {
            for (i = 1; i <= totalcount; i++) {
                $("#li" + i).css("display", "none");
            }
            if (tabid_id > 1) {

                if (parseFloat(tabid_id) + 2 <= parseFloat(totalcount)) {
                    for (i = parseFloat(tabid_id == 2 ? 3 : tabid_id) - 2; i <= parseFloat(tabid_id == 2 ? 3 : tabid_id) + 2; i++) {
                        if (i <= totalcount) {
                            $("#li" + i).css("display", "");
                        }
                    }
                }
                else {
                    for (i = parseFloat(totalcount) - 4; i <= parseFloat(totalcount); i++) {

                        $("#li" + i).css("display", "");
                    }
                }
            }
            else {
                for (i = parseFloat(1); i <= parseFloat(5); i++) {
                    $("#li" + i).css("display", "");
                }
            }
        }
        $('#table1 tr').each(function () {
            if ($(this).context.id.contains("-")) {
                $(this).hide();
            }
        });
        setTabActive("TacFileUploadViewEditorTab");
    };
    String.prototype.contains = function (a) {
        return -1 != this.indexOf(a)
    };
    
</script>
